    "files.watcherExclude": {
        "**/.git/**": true,
        "**/.svn/**": true,
        "**/.hg/**": true,
        "**/CVS/**": true,
        "**/.DS_Store/**": true,
        "**/node_modules/**": true,
        "**/bower_components/**": true,
        "**/deps/**": true,
        "**/.history/**": true
